class Event < ActiveRecord::Base
  belongs_to :owner, :class_name => "User", :foreign_key => "owner_id"
  #belongs_to :attachment, :foreign_key => "source_id"
    
  attr_accessible :org_id, :owner_id, :event_start, 
  :event_end, :event_poi, :event_address, :event_lat, :event_lng, 
  :event_open, :is_valid

  validates_presence_of :org_id, :on => :create
  validates_presence_of :owner_id, :on => :create
  #validates_presence_of :image_type, :on => :create

end
