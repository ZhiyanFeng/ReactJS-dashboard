               *********************************************************************/
               .carousel_bar_area {
               position: absolute;
               bottom: 0px;
               min-width: 70%;
               width: 316px;
               left: 0px;
               right: 0px;
               margin: 0 auto;
               text-align: center;
               min-height: 35px;
