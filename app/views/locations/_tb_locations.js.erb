<% array = [] %>
<% @locations.each do |location| %>
	<% array.push({
		Name: location.location_name,
		Address: location.address,
		City: "#{location.city}, #{location.province}",
		Created: location.created_at.strftime('%F %H:%M'),
		"Member Count": location.member_count,
		"Details": "http://toolbeam.dev/t/ltwaayvk/response?id=#{location.id}"
	}) %>
<% end %>
<%= array.to_json.html_safe %>
