<script>
$('#go_personal').click(function() {
	if( !$("#name").val() 
	|| !$("#street_number").val() 
	|| !$("#address").val() 
	|| !$("#city").val() 
	|| !$("#province").val() 
	|| !$("#postal").val() 
	|| !$("#country").val() ) {
	   if ( !$("#name").val()) {
			$("#name").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#name").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}
		
		if ( !$("#street_number").val()) {
			$("#street_number").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#street_number").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}

		if ( !$("#address").val()) {
			$("#address").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#address").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}
		
		if ( !$("#city").val()) {
			$("#city").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#city").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}
		
		if ( !$("#province").val()) {
			$("#province").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#province").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}
		
		if ( !$("#postal").val()) {
			$("#postal").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#postal").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}
		
		if ( !$("#country").val()) {
			$("#country").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
		} else {
			$("#country").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
		}
	} else {
		str_address = $("#street_number").val()+" "+$("#address").val()+", "+$("#city").val()+", "+$("#province").val()+", "+$("#country").val();
		try {
			codeAddress(str_address);
		} catch(error) {
			console.log(error);
		}
		$("#signup_table_one").hide();
	   	$("#signup_table_two").show();
	}
});

$('#go_organization').click(function() {
   	$("#signup_table_two").hide();
   	$("#signup_table_one").show();
});

$('#finish_registration').click(function() {
   	if( !$("#name").val() 
	|| !$("#street_number").val() 
	|| !$("#address").val() 
	|| !$("#city").val() 
	|| !$("#province").val() 
	|| !$("#postal").val() 
	|| !$("#country").val()
	|| !$("#email").val()
	|| !$("#first_name").val()
	|| !$("#last_name").val()
	|| !$("#password").val()
	|| !$("#confirm_password").val() ) {
		if ( !$("#email").val()) {
				$("#email").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			} else {
				$("#email").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
			}

			if ( !$("#first_name").val()) {
				$("#first_name").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			} else {
				$("#first_name").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
			}

			if ( !$("#last_name").val()) {
				$("#last_name").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			} else {
				$("#last_name").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
			}

			if ( !$("#password").val()) {
				$("#password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			} else {
				$("#password").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
			}

			 if ( !$("#confirm_password").val()) {
				$("#confirm_password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			} else {
				$("#confirm_password").attr("style", "border: 1px solid #dedede; background-color: #efefef;");
			}
		} else if (!validateEmail($("#email").val())) {
			alert("Error: The email address provided is invalid!");
			$("#email").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			$("#email").focus();
		} else if ($("#password").val() != $("#confirm_password").val()) {
			alert("Error: The passwords do not match.");
			$("#password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			$("#confirm_password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			$("#password").focus();
		} else if ($("#password").val().length < 6) {
			$("#password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			$("#confirm_password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			alert("Error: Password must contain at least six characters!");
			$("#password").focus();
		} else if($("#password").val() == $("#email").val() || $("#password").val() == $("#first_name").val() || $("#password").val() == $("#last_name").val()) {
			$("#password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			$("#confirm_password").attr("style", "border: 1px solid #b94a48; background-color: #eed3d7;");
			alert("Error: Password must be different from your name or email!");
			$("#password").focus();
		} else {
			$("#registration_form").submit();
			//console.log("All Good");
		}
	});

	function validateEmail(email) { 
		var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		return re.test(email);
	} 

	geocoder = new google.maps.Geocoder();

  	function codeAddress(address) {
    //In this case it gets the address from an element on the page, but obviously you  could just pass it to the method instead

    geocoder.geocode( { 'address': address}, function(results, status) {
		if (status == google.maps.GeocoderStatus.OK) {
			//In this case it creates a marker, but you can get the lat and lng from the location.LatLng
			//map.setCenter(results[0].geometry.location);
			//var marker = new google.maps.Marker({
			//    map: map, 
			//    position: results[0].geometry.location
			//});
    		$("#lng").val(results[0].geometry.location.lng())
    		$("#lat").val(results[0].geometry.location.lat())
		} else {
			console.log("Geocode was not successful for the following reason: " + status);
		}
		});
  	}
</script>
			
